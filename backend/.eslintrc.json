{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier",
        "prettier/react"        
    ],  
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "off"
    }
}
